{
  "manifest_version": 3,
  "name": "Avalab.aiの生成画像を非表示にするやつ",
  "description": "Avalab.aiの生成画像を非表示とする拡張機能。",
  "version": "1.0",
  "permissions": ["storage"], "icons": {
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://avalab.ai/generate"],
      "js": ["index.js"],
      "css": ["global.css"]
    }
  ]
}
